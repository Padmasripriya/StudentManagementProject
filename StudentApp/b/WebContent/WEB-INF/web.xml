<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>Student Application</display-name>
  <welcome-file-list>
    <welcome-file>student_info.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>studentServlet</servlet-name>
    <servlet-class>com.demo.Student.StudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>studentServlet</servlet-name>
    <url-pattern>/student.htm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>meetingServlet</servlet-name>
    <servlet-class>com.demo.Meeting.MeetingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>meetingServlet</servlet-name>
    <url-pattern>/meeting.htm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logoutServlet</servlet-name>
    <servlet-class>com.demo.Meeting.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logoutServlet</servlet-name>
    <url-pattern>/logoutUser.htm</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>saveServlet</servlet-name>
    <servlet-class>com.demo.Meeting.SaveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>saveServlet</servlet-name>
    <url-pattern>/saveInfo.htm</url-pattern>
  </servlet-mapping>
</web-app>