<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>UserMng</display-name>
  <welcome-file-list>
    <welcome-file>page.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.user.mng.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/validateUser.htm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deleteServlet</servlet-name>
    <servlet-class>com.user.mng.servlet.DeleteUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deleteServlet</servlet-name>
    <url-pattern>/deleteUser.htm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>processUserServlet</servlet-name>
    <servlet-class>com.user.mng.servlet.ProcessUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>processUserServlet</servlet-name>
    <url-pattern>/processUser.htm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>editUserServlet</servlet-name>
    <servlet-class>com.user.mng.servlet.EditUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>editUserServlet</servlet-name>
    <url-pattern>/editUser.htm</url-pattern>
  </servlet-mapping>
  
</web-app>